/******************************************************************************

    Write a program to find the factorial value of any number entered 
    through the keyboard.
         
    Code by Junaid Khan

*******************************************************************************/

#include <iostream>

using namespace std;

int main()
{
    int i , fact=1; 
    cout << "Enter a number to find factorial" << endl;
    cin >> i;
    for (i ; i >= 1; i--) {
        fact *=  i; 
    }
    cout << fact;
}
